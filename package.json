{
  "name": "nestjs-starter-kit",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:api": "node dist/apps/api/main",
    "start:worker": "node dist/apps/worker/main",
    "start:migration": "node dist/apps/migration/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/api/test/jest-e2e.json",
    "compodoc": "npx @compodoc/compodoc -p tsconfig.json -s",
    "docker:build": "docker-compose -f docker/build/production.yml build"
  },
  "dependencies": {
    "@atomictech/xlsx-write-stream": "^2.0.2",
    "@casl/ability": "^6.5.0",
    "@nestjs/apollo": "^11.0.6",
    "@nestjs/cache-manager": "^1.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/cqrs": "^10.0.3",
    "@nestjs/graphql": "^11.0.6",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/microservices": "^9.4.2",
    "@nestjs/mongoose": "^9.2.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/throttler": "^4.0.0",
    "@supercharge/request-ip": "^1.2.0",
    "amqp-connection-manager": "^4.1.13",
    "amqplib": "^0.10.3",
    "axios": "^1.4.0",
    "cache-manager": "^5.2.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "config": "^3.3.9",
    "consistent-hash": "^1.1.1",
    "cookie-parser": "^1.4.6",
    "dataloader": "^2.2.2",
    "dotenv": "^16.0.3",
    "exceljs": "^4.3.0",
    "fastq": "^1.15.0",
    "file-type": "^18.5.0",
    "graphql": "^16.6.0",
    "graphql-type-json": "^0.3.2",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "minio": "^7.1.1",
    "moment-timezone": "^0.5.43",
    "mongoose": "^7.2.1",
    "passport-jwt": "^4.0.1",
    "password-hash": "^1.2.2",
    "pixl-cache": "^1.0.6",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "slugify": "^1.6.6",
    "tiny-typed-emitter": "^2.1.0",
    "v8": "^0.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.4.3",
    "@types/amqplib": "^0.10.1",
    "@types/config": "^3.3.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.1",
    "@types/lodash": "^4.14.195",
    "@types/mime-types": "^2.1.1",
    "@types/multer": "^1.4.7",
    "@types/node": "18.16.12",
    "@types/passport-jwt": "^3.0.8",
    "@types/password-hash": "^1.2.21",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "dpdm": "^3.13.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/dal(|/.*)$": "<rootDir>/libs/dal/src/$1",
      "^@app/app-generic(|/.*)$": "<rootDir>/libs/app-generic/src/$1",
      "^@app/shared(|/.*)$": "<rootDir>/libs/shared/src/$1",
      "^@app/core(|/.*)$": "<rootDir>/libs/core/src/$1"
    }
  },
  "prettier": {
    "printWidth": 120
  }
}
